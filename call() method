function displayDetails() {
  console.log("Id is " + this.id);
  console.log("Name is " + this.name);
}

let student = {
  id: 1,
  name: "Nitesh"
}

let student1 = {
  id: 2,
  name: "Madhav"
}

displayDetails.call(student);
displayDetails.call(student1);

// Id is 1 
Name is Nitesh

Id is 2
Name is Madhav
